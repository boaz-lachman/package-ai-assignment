{
  "expo": {
    "name": "package-ai-assignment",
    "slug": "package-ai-assignment",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/package_ai_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/package_ai_icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.boaz606.packageaiassignment",
      "statusBar": {
        "backgroundColor": "#f5f5f5",
        "barStyle": "dark-content"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-build-properties",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/Package_ai_Logo.png",
          "imageWidth": 200
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      ["expo-localization", {
        "fallbackLocale": "en",
        "availableLocales": ["en", "he"]
      }]
    ],
    "extra": {
      "supportsRTL": true,
      "eas": {
        "projectId": "0d36318c-3d8d-478d-ab57-20fd78e10e4f"
      }
    }
  }
}
